{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "google/apiclient": "^2.18",
        "google/cloud-firestore": "^1.50",
        "guzzlehttp/guzzle": "^7.2",
        "kreait/firebase-php": "^7.6",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laraveldaily/laravel-charts": "^0.1.29",
        "mongodb/laravel-mongodb": "^5.5",
        "mongodb/mongodb": "^2.1",
        "paydunya/paydunya": "^1.0",
        "paypal/paypal-checkout-sdk": "^1.0",
        "spatie/laravel-medialibrary": "^10.7",
        "stripe/stripe-php": "^13.16",
        "yajra/laravel-datatables-oracle": "^10.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "doctrine/dbal": "^3.10",
        "fakerphp/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^7.0",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.18",
        "laravel/ui": "^4.2",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },"files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
